@using Product_manage_RAHUL_SATA.Models
@{
    Layout = null;
}
@model RegisterViewModel

@{
    ViewBag.Title = "Sign up";
}
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="@Url.Content("../Content/IMAGE_FILES/login_page_images/icons/favicon.ico" )" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("../Content/VENDOR_FILES/login_page_vendor/bootstrap/css/bootstrap.min.css")">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/FONT_FILES/login_page_fonts/font-awesome-4.7.0/css/font-awesome.min.css")">

    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/FONT_FILES/login_page_fonts/font-awesome-4.7.0/css/font-awesome.css")">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Content/FONT_FILES/login_page_fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Content/VENDOR_FILES/login_page_vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Content/VENDOR_FILES/login_page_vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Content/VENDOR_FILES/login_page_vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Content/VENDOR_FILES/login_page_vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Content/VENDOR_FILES/login_page_vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/CSS_FILES/Login_page_css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Content/CSS_FILES/Login_page_css/main.css">
    @Styles.Render("~/Content/CSS_FILES/Login_page_css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<div class="limiter">
    <div class="container-login100" style="background-image: url('../Content/IMAGE_FILES/login_page_images/bg-01.jpg');">
        <div class="wrap-login100 p-l-110 p-r-110 p-t-62 p-b-33">

            <span class="login100-form-title p-b-53">
                Register with
            </span>
            <div class="btn-face m-b-20">
                <hr>
                @Html.Partial("_ExternalLoginsListPartial", new ExternalLoginListViewModel { ReturnUrl = ViewBag.ReturnUrl })
            </div>
            @using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
            {
                @Html.AntiForgeryToken()

                @Html.ValidationSummary("", new { @class = "text-danger" })
                <div class="p-t-31 p-b-9">
                    <span class="txt1">
                        Username
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Email is required">
                    <input class="input100" type="text" name="Email">
                    <span class="focus-input100"></span>
                </div>
                <div class="p-t-13 p-b-9">
                    <span class="txt1">
                        Password
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Password is required">
                    <input class="input100" type="password" name="Password">
                    <span class="focus-input100"></span>
                </div>
                <div class="p-t-13 p-b-9">
                    <span class="txt1">
                        Confirm Password
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Password is required">
                    <input class="input100" type="password" name="ConfirmPassword">
                    <span class="focus-input100"></span>
                </div>
                <div class="container-login100-form-btn m-t-17">
                    <button class="login100-form-btn">
                        Sign Up
                    </button>
                </div>
                <div class="w-full text-center p-t-55">
                    <span class="txt2">
                        Already a member?
                    </span>
                    <a href="@Url.Action("Login", "Account" )" class="txt2 bo1">Sign in now</a>

                </div>
            }
        </div>
    </div>
</div>



<div id="dropDownSelect1"></div>

<!--===============================================================================================-->
<script src="../Content/VENDOR_FILES/login_page_vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
<script src="../Content/VENDOR_FILES/login_page_vendor/animsition/js/animsition.min.js"></script>
<!--===============================================================================================-->
<script src="../Content/VENDOR_FILES/login_page_vendor/bootstrap/js/popper.js"></script>
<script src="../Content/VENDOR_FILES/login_page_vendor/bootstrap/js/bootstrap.min.js"></script>
<!--===============================================================================================-->
<script src="../Content/VENDOR_FILES/login_page_vendor/select2/select2.min.js"></script>
<!--===============================================================================================-->
<script src="../Content/VENDOR_FILES/login_page_vendor/daterangepicker/moment.min.js"></script>
<script src="../Content/VENDOR_FILES/login_page_vendor/daterangepicker/daterangepicker.js"></script>
<!--===============================================================================================-->
<script src="../Content/VENDOR_FILES/login_page_vendor/countdowntime/countdowntime.js"></script>
<!--===============================================================================================-->
<script src="../Content/JS_FILES/login_page_js/main.js"></script>

